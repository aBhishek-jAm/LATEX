\documentclass{article}
\usepackage{cite}
\title{\textbf{CITATIONS AND REFERENCES}}
\begin{document}
\maketitle
\section{First paragraph}
this is frst para its includes \cite{cite1,cite2,cite3}
\section{Second paragraph}
this is scnd para its includes \cite{cite4,cite5,cite1}
\begin{thebibliography}{5}
\bibitem{cite1}a1
\bibitem{cite2}a2
\bibitem{cite3}a3
\bibitem{cite4}a4
\bibitem{cite5}a5
\end{thebibliography}
\end{document}
